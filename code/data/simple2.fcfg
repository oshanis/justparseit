S[sem = <and (and (Actor(?actor)) (?action)) (Purpose(?purpose))>] -> Actor[sem = ?actor] Action[sem = ?action] Purpose[sem = ?purpose]
Actor[sem = ?np] -> NP[sem = ?np]
Action[sem = <and (Action (?v)) (ActedOn (?np))>] -> V[sem = ?v] NP[sem = ?np]
Purpose[sem = ?np]  -> 'for' NP[sem = ?np]
Purpose[sem = ?vp] -> 'to' VP[sem = ?vp]
NP[sem = <(and (NP (?np)) (PP (?pp)))>] -> NP[sem = ?np] PP[sem = ?pp]
PP[sem = <(and (P (?p)) (NP (?np)))>] -> P[sem = ?p] NP[sem = ?np]
N[sem = <(and (N1 (?n1)) (N2 (?n2)))>] -> N[sem = ?n1] N[sem = ?n2]
V[sem = <(and (V1 (?v1)) (V2 (?v2)))>] -> V[sem = ?v1] V[sem = ?v2]
